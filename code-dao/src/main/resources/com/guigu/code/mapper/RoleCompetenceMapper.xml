<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.guigu.code.mapper.RoleCompetenceMapper">
    <delete id="delete">
        DELETE FROM role_competence WHERE role_id=#{param1}
    </delete>
    <insert id="inserts">
        INSERT INTO role_competence VALUE(NULL,#{param1},#{param2});
    </insert>
</mapper>