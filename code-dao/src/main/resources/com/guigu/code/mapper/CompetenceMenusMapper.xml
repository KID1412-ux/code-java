<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.guigu.code.mapper.CompetenceMenusMapper">
    <delete id="deletebycomid" parameterType="java.lang.Integer">
        DELETE FROM competence_menus WHERE competence_id=#{param1}
    </delete>
    <insert id="insert">
        INSERT INTO competence_menus VALUE(NULL,#{param1},#{param2});
    </insert>
</mapper>